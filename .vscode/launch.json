{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "flutter_clean_architecture",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "flutter_clean_architecture (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "flutter_clean_architecture (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Flutter Web (Chrome with no restrictions)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-browser-flag",
        "--disable-web-security",
        "--web-browser-flag",
        "--user-data-dir=/tmp/chrome_dev"
      ]
    }
  ]
}
